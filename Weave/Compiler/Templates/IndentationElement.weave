@namespace Weave.Compiler
@methodname RenderIndentationElement
@model IndentationElement
@using Weave.Expressions

{{if this.lastIndentation.Length != model.Indentation.Length - this.amountToSubtract}}
    indentation = originalIndentation{{if model.Indentation.Length - this.amountToSubtract > 0}} + {{= ToLiteral(model.Indentation.Substring(0, model.Indentation.Length - this.amountToSubtract)) }}{{/if}};
